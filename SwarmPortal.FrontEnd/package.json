    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@types/jasmine": "~4.0.0",
    "@types/url-parse": "1.4.4",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.7.2"
  }
}
