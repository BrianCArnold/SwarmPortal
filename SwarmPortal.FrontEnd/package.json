{
  "name": "@swarm-portal/frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run generate-client-sdk",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate-client-sdk": "mkdir -p src/app/api && openapi-generator-cli generate -i http://localhost:5109/swagger/v1/swagger.json -g typescript-angular -o src/app/api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "ag-grid-angular": "^27.3.0",
    "ag-grid-community": "^27.3.0",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-oauth2-oidc-jwks": "^13.0.1",
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0",
    "masonry-layout": "^4.2.2",
    "ngx-cookie-service": "^14.0.0",
    "ngx-masonry": "^13.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "url-parse": "^1.5.10",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular/cli": "~14.0.1",
